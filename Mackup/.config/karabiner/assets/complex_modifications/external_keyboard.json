{
    "title": "External Keyboard, Vendor 7847",
    "rules": [
        {
            "description": "Alt <-> Command, vendor 7847",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 7847
                                }
                            ]
                        }
                    ],
                    "type": "basic",
                    "from": {
                        "key_code": "left_option",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_command"
                        }
                    ]
                },
                {
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 7847
                                }
                            ]
                        }
                    ],
                    "type": "basic",
                    "from": {
                        "key_code": "right_option",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_command"
                        }
                    ]
                }
            ]
        },
        {
            "description": "left_control -> hyper; application -> right_option, vendor 7847",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 7847
                                }
                            ]
                        }
                    ],
                    "type": "basic",
                    "from": {
                        "key_code": "left_control",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_shift",
                            "modifiers": [
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    ]
                },
                {
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 7847
                                }
                            ]
                        }
                    ],
                    "type": "basic",
                    "from": {
                        "key_code": "application",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_option"
                        }
                    ]
                }
            ]
        }
    ]
}
