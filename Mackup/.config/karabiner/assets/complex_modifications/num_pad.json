    "title": "Numeric Keypad",
    "rules": [
        {
            "description": "Numeric Keypad Trigger [a as trigger key]",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "a"
                            },
                            {
                                "key_code": "spacebar"
                            }
                        ],
                        "simultaneous_options": {
                            "key_down_order": "strict",
                            "key_up_order": "strict_inverse",
                            "to_after_key_up": [
                                {
                                    "set_variable": {
                                        "name": "numeric_keypad_mode",
                                        "value": 0
                                    }
                                }
                            ]
                        },
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "numeric_keypad_mode",
                                "value": 1
                            }
                        },
                        {
                            "key_code": "keypad_0"
                        }
                    ],
                    "parameters": {
                        "basic.simultaneous_threshold_milliseconds": 500
                    }
                },
